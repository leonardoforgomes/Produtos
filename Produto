package streams;

public class Produto {

	final String nomeProduto; // Quando não muda o cenário, utiliza-se o 'final'
	final double preco;
	final double desconto;
	final double frete;
	
	public Produto(String nomeProduto, double preco, double desconto, double frete) {
		this.nomeProduto = nomeProduto;
		this.preco = preco;
		this.desconto = desconto;
		this.frete = frete;
	}
}
